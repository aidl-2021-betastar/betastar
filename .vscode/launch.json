        {
            "name": "Move To Beacon Simple (MovePPO)",
            "type": "python",
            "request": "launch",
            "module": "betastar",
            "args": [ 
                "run",
                "--agent",
                "move_ppo",
                "--environment",
                "SC2MoveToBeaconSimple-v0",
                "--num-workers",
                "4",
                "--learning-rate",
                "0.0001",
                "--use-ppo",
                "--use-gae",
                "--clip-range",
                "0.25",
                "--unroll-length",
                "60",
